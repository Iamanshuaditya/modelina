{
  "$id": "Address",
  "description": "This object contains all types of MetaModel generations",
  "type": "object",
  "properties": {
    "street_name": {
      "type": "string"
    },
    "house_number": {
      "type": "number"
    },
    "marriage": {
      "type": "boolean",
      "description": "Status if marriage live in given house"
    },
    "members": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        },
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ]
    },
    "array_type": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "nested_property": {
            "type": "string"
          },
          "nested_number": {
            "type": "number"
          },
          "nested_bool": {
            "type": "boolean"
          }
        }
      }
    },
    "nestedArray": {
      "type": "array",
      "items": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "customFormat": {
      "type": "string",
      "format": "custom-date-time"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "uuid": {
      "type": "string",
      "format": "uuid"
    },
    "additionalConstraints": {
      "type": "string",
      "minLength": 5,
      "maxLength": 10
    },
    "nestedObject": {
      "type": "object",
      "properties": {
        "nested_property": {
          "type": "object",
          "properties": {
            "nested_test": {
              "type": "string"
            }
          }
        }
      }
    },
    "enumTest": {
      "enum": ["test", "test2", "test3", "test4"]
    },
    "houseType": {
      "$ref": "#/$defs/housing-types"
    },
    "roofType": {
      "$id": "TypeOfRoof",
      "enum": ["tile", "straw", "wood", "metal"]
    },
    "dateOfBirth": {
      "type": "string",
      "format": "date-time"
    },
    "bigNumber": {
      "type": "number",
      "minimum": 1000000000
    },
    "lowNumber": {
      "type": "number",
      "maximum": 0.000001
    }
  },
  "patternProperties": {
    "^S.*test$": {
      "type": "string"
    }
  },
  "required": [
    "house_number",
    "array_type"
  ],
  "$defs": {
    "housing-types": {
      "$id": "HousingType",
      "enum": ["detached", "terraced", "bungalow", "flat"]
    }
  }
}
